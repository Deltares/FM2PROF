    NegativeDepth         = 0                   # GridpointsOnBranches Output Negative depth
    NetSolarRad           = 0                   # GridpointsOnBranches Output Net incident solar radiation
    NoIteration           = 0                   # GridpointsOnBranches Output Number of iterations
    Lateral1D2D           = 0                   # GridpointsOnBranches Output Lateral Discharge from 2d to 1d
    RadFluxClearSky       = 0                   # GridpointsOnBranches Output Radiation flux for clear sky condition
